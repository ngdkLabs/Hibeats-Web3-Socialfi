# Sequence Configuration
VITE_SEQUENCE_PROJECT_ACCESS_KEY=your_sequence_project_access_key_here
VITE_SEQUENCE_WAAS_CONFIG_KEY=your_sequence_waas_config_key_here

# Somnia Configuration with Real-time Support
VITE_SOMNIA_RPC_URL=https://dream-rpc.somnia.network
# Somnia Testnet WebSocket endpoint with correct path
VITE_SOMNIA_WS_URL=wss://dream-rpc.somnia.network/ws

# Real-time Updates Configuration (Enabled)
VITE_ENABLE_REALTIME_POLLING=true
VITE_ENABLE_WEBSOCKET=true
VITE_POLLING_INTERVAL=15000
VITE_SOMNIA_DATASTREAM_API_KEY=your_somnia_datastream_api_key_here

# Performance Settings
VITE_RUN_INTEGRATION_TESTS=false

# Social login configuration (optional)
VITE_GOOGLE_CLIENT_ID=your_google_client_id_here
VITE_APPLE_CLIENT_ID=your_apple_client_id_here

# Suno API Configuration
VITE_SUNO_API_KEY=your_suno_api_key_here
# Use a CORS proxy if direct API calls fail (uncomment and configure if needed)
# VITE_SUNO_PROXY_URL=https://cors-anywhere.herokuapp.com/https://api.suno.ai
# Alternative proxy options:
# VITE_SUNO_PROXY_URL=https://api.allorigins.win/raw?url=https://api.suno.ai
# Or set up your own proxy server

# Pinata IPFS Configuration
VITE_PINATA_API_KEY=your_pinata_api_key_here
VITE_PINATA_API_SECRET=your_pinata_api_secret_here
VITE_PINATA_API_JWT=your_pinata_jwt_token_here

# AWS Bedrock AI Configuration (for YourVibe AI features)
# Option 1: Direct Bedrock access (requires AWS credentials)
VITE_AWS_REGION=us-east-1
# Option 2: API Gateway proxy (recommended for frontend)
VITE_BEDROCK_API_ENDPOINT=https://your-api-gateway-url.execute-api.us-east-1.amazonaws.com/prod
VITE_BEDROCK_API_KEY=your_api_gateway_key_here

# NEVER use a wallet with real funds! (For DataStream writes only)
VITE_PRIVATE_KEY=0x_your_private_key_here_for_development_only

# Playlist Schema IDs (Computed from Somnia DataStream)
VITE_PLAYLIST_SCHEMA_ID=your_playlist_schema_id_here
VITE_PLAYLIST_INTERACTION_SCHEMA_ID=your_playlist_interaction_schema_id_here

